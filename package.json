{
  "name": "salon-reservation",
  "private": true,
  "description": "A Salon reservation Site for Gatsby",
  "version": "0.1.0",
  "license": "0BSD",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "setup": "node ./bin/setup.js",
    "cy:open": "cypress open",
    "test:e2e:CI": "cypress run --reporter mochawesome --reporter-options 'reportDir=test/cypress/report/mochawesome-report,overwrite=false,html=false,json=true,timestamp=mmddyyyy_HHMMss'",
    "test:e2e:report:merge": "mochawesome-merge 'test/cypress/report/mochawesome-report/*.json' > test/cypress/report/mochawesome.json"
  },
  "dependencies": {
    "@date-io/date-fns": "1.3.13",
    "@fortawesome/fontawesome-svg-core": "1.2.35",
    "@fortawesome/free-brands-svg-icons": "5.15.3",
    "@fortawesome/free-regular-svg-icons": "5.15.3",
    "@fortawesome/free-solid-svg-icons": "5.15.3",
    "@fortawesome/react-fontawesome": "0.1.14",
    "@material-ui/core": "4.11.4",
    "@material-ui/icons": "4.11.2",
    "@material-ui/lab": "4.0.0-alpha.58",
    "@material-ui/pickers": "3.3.10",
    "@sentry/react": "6.3.6",
    "@sentry/serverless": "6.3.6",
    "@sentry/tracing": "6.3.6",
    "aws-amplify": "3.4.0",
    "date-fns": "2.21.3",
    "gatsby": "3.5.0",
    "gatsby-image": "3.5.0",
    "gatsby-plugin-google-tagmanager": "3.5.0",
    "gatsby-plugin-image": "1.5.0",
    "gatsby-plugin-material-ui": "3.0.1",
    "gatsby-plugin-sharp": "3.5.0",
    "gatsby-source-contentful": "5.5.0",
    "gatsby-source-filesystem": "3.5.0",
    "gatsby-transformer-sharp": "3.5.0",
    "google-map-react": "2.1.9",
    "moment": "2.29.1",
    "notistack": "1.0.7",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-feather": "2.0.9",
    "react-helmet": "6.1.0",
    "react-hook-form": "6.15.8",
    "react-responsive": "8.2.0",
    "uuid": "8.3.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Thirosue/booking"
  },
  "bugs": {
